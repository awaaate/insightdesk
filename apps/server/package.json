{
  "name": "server",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "tsc && tsc-alias",
    "check-types": "tsc --noEmit",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
    "dev": "bun run --hot src/index.ts",
    "start": "bun run dist/src/index.js",
    "db:local": "turso dev --db-file local.db",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "concurrently \"bun src/db/seed-insights.ts\" \"bun src/db/seed-intentions.ts\" \"bun src/db/seed-sentiment-levels.ts\"",
    "cli": "bun src/cli/index.ts",
    "clean": "rm -rf node_modules && rm -rf dist && rm -rf .turbo"
  },
  "template": "docker",
  "templateVersion": "1.3.15",
  "dependencies": {
    "@ai-sdk/google": "^2.0.4",
    "@ai-sdk/openai": "^2.0.0",
    "@ai-sdk/perplexity": "^2.0.0",
    "@bull-board/api": "^6.9.6",
    "@bull-board/hono": "^6.9.6",
    "@hono/node-server": "^1.14.1",
    "@hono/trpc-server": "^0.3.4",
    "@libsql/client": "^0.14.0",
    "@openrouter/ai-sdk-provider": "^1.1.2",
    "@trpc/client": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "@types/prompt": "^1.1.9",
    "add": "^2.0.6",
    "ai": "^5.0.10",
    "bullmq": "^5.52.2",
    "chalk": "^5.4.1",
    "cli-table3": "^0.6.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.43.1",
    "handlebars": "^4.7.8",
    "hono": "^4.7.6",
    "ioredis": "^5.6.1",
    "ora": "^8.2.0",
    "pg": "^8.15.6",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "postgres": "^3.4.7",
    "prompts": "^2.4.2",
    "ramda": "^0.31.3",
    "remeda": "^2.30.0",
    "type-fest": "^4.41.0",
    "xlsx": "^0.18.5",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@cubejs-backend/postgres-driver": "^1.3.15",
    "@cubejs-backend/server": "^1.3.15",
    "@types/bun": "^1.2.6",
    "@types/node": "^24.0.13",
    "@types/pg": "^8.15.1",
    "@types/prompts": "^2.4.9",
    "@types/ramda": "^0.31.0",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.1",
    "tsc-alias": "^1.8.11",
    "typescript": "^5.8.2"
  }
}